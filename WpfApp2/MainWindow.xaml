<Window x:Class="WpfApp2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        WindowStyle="ToolWindow">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="60"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <StackPanel  Orientation="Horizontal" Grid.Row="0">
            <Button Content="Назад" Click="Back_Click"/>
            <Button Content="1" Click="A_Click"/>
            <Button Content="2" Click="B_Click"/>
            <Button Content="3" Click="C_Click"/>
            <Button Content="4" Click="D_Click"/>
            <Button Content="5" Click="J_Click"/>
            <Button Content="Вперед" Click="Next_Click"/>
        </StackPanel>
        <Frame NavigationUIVisibility="Hidden" x:Name="MyFrame" Grid.Row="1"/>
    </Grid>
</Window>
